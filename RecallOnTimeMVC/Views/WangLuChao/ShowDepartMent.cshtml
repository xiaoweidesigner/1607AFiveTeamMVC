@model IEnumerable<RecallOnTimeMVC.Models.DepartMent>

@{
    ViewBag.Title = "ShowDepartMent";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ShowDepartMent</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.D_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Num)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.D_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Num)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DId }) |
